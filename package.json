{
	"name": "obsidian-related-notes-view",
	"version": "1.0.0",
	"description": "Obsidian plugin to display related notes in a collapsible sidebar view based on tags and links.",
	"author": "KiNoTch.",
	"license": "MIT",
	"main": "main.js",
	"scripts": {
	  "build": "esbuild src/main.ts --bundle --outfile=main.js --target=es2020 --minify",
	  "dev": "esbuild src/main.ts --bundle --outfile=main.js --target=es2020 --watch"
	},
	"devDependencies": {
	  "esbuild": "^0.20.0",
	  "@types/node": "^20.0.0",
	  "obsidian": "latest"
	},
	"keywords": [
	  "obsidian-plugin",
	  "related-notes",
	  "sidebar",
	  "tags",
	  "links",
	  "markdown",
	  "note-taking"
